  "constellations": [
    {
      "name": "Chained Reactions",
      "description": "Attacks and Skills have a certain chance to summon sparks that bombard opponents, dealing DMG equal to 120% of <span style=\"color: #FFD780FF;\">Sparks 'n' Splash</span>'s DMG."
